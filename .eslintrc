{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:security/recommended",
    "prettier",
    "plugin:jest/recommended"
  ],
  "plugins": ["prettier", "jest", "security"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"]
  }
}
